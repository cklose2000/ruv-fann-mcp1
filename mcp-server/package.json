{
  "name": "ruv-fann-mcp-server",
  "version": "0.1.0",
  "description": "MCP server leveraging ruv-FANN for cognitive enhancement of Claude Code",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest tests/unit",
    "test:integration": "node tests/integration/end-to-end-test.js",
    "test:accuracy": "node tests/integration/accuracy-validation-test.js",
    "test:business-value": "node tests/integration/business-value-test.js",
    "test:performance": "NODE_OPTIONS='--experimental-vm-modules' jest tests/performance --testTimeout=30000",
    "test:all": "npm run build && npm run test:integration && npm run test:accuracy && npm run test:business-value",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:report": "node tests/utils/generate-report.js",
    "test:dashboard": "node tests/reports/test-results-dashboard.js",
    "test:mock-backend": "node ../mock-gcp-backend.js",
    "benchmark": "node tests/performance/benchmark.js",
    "lint": "eslint src --ext .ts",
    "train": "node train-network.js",
    "business-metrics": "node business-metrics-test.js",
    "developer-time": "node developer-time-savings-test.js",
    "metrics-dashboard": "node metrics-dashboard.js",
    "time-dashboard": "node time-savings-dashboard.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.0",
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^9.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/better-sqlite3": "^7.6.8",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "express": "^4.18.2",
    "socket.io": "^4.5.0"
  },
  "keywords": ["mcp", "ai", "neural-network", "executive-function"],
  "author": "",
  "license": "MIT"
}